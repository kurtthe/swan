{
  "expo": {
    "name": "Swan",
    "slug": "Swan",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c4c86bf4-7b58-4b5c-bdab-d2582e2a30de"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@react-native-firebase/app"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trade.Burdens",
      "buildNumber": "1",
      "googleServicesFile": "./GoogleAnalytics/GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Swan to access your camera to take photos of the products for any requirement in your purchase",
        "NSPhotoLibraryUsageDescription": "Allow Swan to access your photos to select photos and then send a requirement of your  purchase",
        "UIUserInterfaceStyle": "Light"
      }
    },
    "android": {
      "package": "com.tradetrak.Burdens",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "versionCode": 57,
      "googleServicesFile": "./GoogleAnalytics/google-services.json"
    },
    "description": "Coded by Digital Basis",
    "extra": {
      "eas": {
        "projectId": "c4c86bf4-7b58-4b5c-bdab-d2582e2a30de"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
